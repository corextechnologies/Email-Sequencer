{
  "expo": {
    "name": "Email Marketing - Bobos.ai",
    "slug": "email-marketing-bobos",
    "version": "1.1",
    "orientation": "portrait",
    "userInterfaceStyle": "light",
    "assetBundlePatterns": [
      "assets/**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "infoPlist": {
        "NSContactsUsageDescription": "This app needs access to your contacts to import them for email marketing."
      },
      "bundleIdentifier": "com.anonymous.emailmarketingbobos"
    },
    "android": {
      "adaptiveIcon": {
        "backgroundColor": "#FFFFFF"
      },
      "permissions": [
        "android.permission.READ_CONTACTS",
        "android.permission.INTERNET",
        "android.permission.ACCESS_NETWORK_STATE"
      ],
      "package": "com.anonymous.emailmarketingbobos",
      "versionCode": 2
    },
    "web": {
      "bundler": "metro"
    },
    "extra": {
      "eas": {
        "projectId": "733209ab-2e49-4c7f-803b-e857edbae25f"
      }
    },
    "plugins": [
      [
        "expo-build-properties",
        {
          "android": {
            "usesCleartextTraffic": true
          }
        }
      ]
    ]
  }
}