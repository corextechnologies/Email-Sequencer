{
  "name": "email-marketing-bobos-backend",
  "version": "1.0.0",
  "description": "Email Marketing Platform Backend",
  "main": "dist/index.js",
  "scripts": {
    "build": "tsc",
    "start": "node dist/index.js",
    "start:prod": "pm2 start ecosystem.config.js --env production",
    "stop:prod": "pm2 stop ecosystem.config.js",
    "restart:prod": "pm2 restart ecosystem.config.js",
    "reload:prod": "pm2 reload ecosystem.config.js",
    "delete:prod": "pm2 delete ecosystem.config.js",
    "logs:prod": "pm2 logs",
    "monit:prod": "pm2 monit",
    "dev": "nodemon src/index.ts",
    "migrate": "node dist/database/migrate.js",
    "migrate:prod": "NODE_ENV=production node dist/database/migrate.js",
    "migrate:012": "node run-012-migration.js",
    "seed": "node dist/database/seed.js",
    "seed:prod": "NODE_ENV=production node dist/database/seed.js",
    "test": "jest",
    "worker": "node dist/workers/sequencer.js",
    "worker:dev": "ts-node src/workers/sequencer.ts",
    "worker:prod": "pm2 start ecosystem.config.js --only email-sequencer-worker --env production",
    "postinstall": "npm run build",
    "prestart": "npm run build"
  },
  "keywords": [
    "email",
    "marketing",
    "api"
  ],
  "author": "Bobos.ai",
  "license": "MIT",
  "dependencies": {
    "@google/generative-ai": "^0.24.1",
    "@react-navigation/native-stack": "^7.3.26",
    "@types/multer": "^2.0.0",
    "all": "^0.0.0",
    "bcryptjs": "^3.0.2",
    "cors": "^2.8.5",
    "csv-parser": "^3.2.0",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "handlebars": "^4.7.8",
    "helmet": "^7.0.0",
    "imap": "^0.8.19",
    "joi": "^17.9.2",
    "jsonwebtoken": "^9.0.2",
    "luxon": "^3.5.0",
    "mailparser": "^3.7.5",
    "morgan": "^1.10.0",
    "multer": "^2.0.2",
    "node-fetch": "^2.7.0",
    "nodemailer": "^6.10.1",
    "pg": "^8.11.3",
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "@types/bcryptjs": "^2.4.6",
    "@types/cors": "^2.8.13",
    "@types/express": "^4.17.17",
    "@types/helmet": "^0.0.48",
    "@types/imap": "^0.8.42",
    "@types/jest": "^29.5.4",
    "@types/jsonwebtoken": "^9.0.2",
    "@types/luxon": "^3.7.1",
    "@types/mailparser": "^3.4.6",
    "@types/morgan": "^1.9.4",
    "@types/node": "^20.19.13",
    "@types/nodemailer": "^7.0.1",
    "@types/pg": "^8.10.2",
    "@types/supertest": "^2.0.16",
    "jest": "^29.6.2",
    "nodemon": "^3.0.1",
    "pm2": "^5.3.0",
    "supertest": "^7.0.0",
    "ts-jest": "^29.1.1",
    "ts-node": "^10.9.1",
    "typescript": "^5.9.3"
  }
}
